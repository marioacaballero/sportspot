{
  "expo": {
    "runtimeVersion": "1.0.2",
    "name": "SpotSport",
    "slug": "spotsport",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/spotsportadaptative.png",
    "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/spotsport.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.aythenapp.spotsportfinal",
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotsportadaptative.png",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "googleServicesFile": "./utils/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotsportadaptative.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aythenapp.spotsport",
      "config": {
        "googleMaps": {
          "apiKey": "@env.GOOGLE_MAPS_API_KEY"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "spotsport.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "4f6d3b77-0567-441d-8b63-8d0d9373c7dd"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ac124ba8-62c5-44ac-ab0e-8da7a8b8e646"
    },
    "owner": "azulschiaffino2",
    "scheme": "spotsport"
  }
}
