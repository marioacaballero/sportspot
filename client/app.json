{
  "expo": {
    "runtimeVersion": "1.0.2",
    "name": "SpotSport",
    "slug": "mytreenueva",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/spotsport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotsport.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aythenapp.spotsport"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotsportadaptative.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aythenapp.spotsport",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "@env.GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "296deecb-db9a-47bc-9d00-4f8bd96480ba"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ac124ba8-62c5-44ac-ab0e-8da7a8b8e646"
    },
    "owner": "azschiaffino",
    "scheme": [
      "spotsport"
    ]
  }
}
