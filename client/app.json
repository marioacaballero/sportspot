{
  "expo": {
    "runtimeVersion": "1.0.2",
    "name": "SpotSport",
    "slug": "spotsport",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/spotsport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotsport.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aythenapp.spotsport"
    },
    "android": {
      "googleServicesFile": "./utils/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotsportadaptative.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aythenapp.spotsport",
      "versionCode": 2,
      "config": {
        "googleMaps": {
          "apiKey": "@env.GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "d7893a40-5e4b-4559-8e1a-1afeb96df0e5"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ac124ba8-62c5-44ac-ab0e-8da7a8b8e646"
    },
    "owner": "kraa",
    "scheme": ["spotsport"]
  }
}
