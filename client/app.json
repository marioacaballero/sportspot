{
  "expo": {
    "runtimeVersion": "1.0.2",
    "name": "SpotSport",
    "slug": "spotsport",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/spotsport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotsport.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aythenapp.spotsport"
    },
    "android": {
      "googleServicesFile": "./utils/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotsportadaptative.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aythenapp.spotsport",
      "versionCode": 3,
      "config": {
        "googleMaps": {
          "apiKey": "@env.GOOGLE_MAPS_API_KEY"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "example.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "3b77cef6-ebe6-4d36-b616-c391d890f0bb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ac124ba8-62c5-44ac-ab0e-8da7a8b8e646"
    },
    "owner": "azulschiaffino",
    "scheme": "spotsport"
  }
}
